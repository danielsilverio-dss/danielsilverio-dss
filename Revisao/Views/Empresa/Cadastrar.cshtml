@model Revisao.Models.Empresa
@{
    ViewBag.Title = "Cadastrar";
}

<h2>Cadastar</h2>

@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(e => e.Cnpj)
        @Html.TextBoxFor(e => e.Cnpj, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(e => e.RazaoSocial)
        @Html.TextBoxFor(e => e.RazaoSocial, new { @class = "form-control" })
    </div>
    <input type="submit" value="Salvar" class="btn btn-primary" />
}